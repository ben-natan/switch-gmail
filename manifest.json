{
    "manifest_version": 3,
    "name": "SwitchGmail",
    "version": "1.0",
    "description": "Switch between 2 Gmail accounts",
    "icons": {
        "16": "icons/s_16.png",
        "32": "icons/s_32.png",
        "48": "icons/s_48.png",
        "128": "icons/s_128.png"
    },
    "action": {
      "default_title": "Switch"
    },
    "permissions": [
        "tabs", "scripting", "activeTab"
    ],
    "host_permissions": ["https://*/*"],
    "background": {
      "service_worker": "background.js"
    }
}
